<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:Metasia.Editor.ViewModels.Inspector"
             mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="450"
             x:Class="Metasia.Editor.Views.Inspector.ClipSettingPaneView"
			 x:DataType="vm:ClipSettingPaneViewModel"
             Padding="10">
    <Border Background="LightGreen" VerticalAlignment="Center" HorizontalAlignment="Stretch" Padding="10">
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Stretch">
	    <TextBlock Text="{Binding DebugTest}"></TextBlock>
        </StackPanel>
    </Border>
</UserControl>
