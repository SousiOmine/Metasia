<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:Metasia.Editor.ViewModels"
             xmlns:views="clr-namespace:Metasia.Editor.Views"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Metasia.Editor.Views.TimelineParentView"
             x:DataType="vm:TimelineParentViewModel">
    <DockPanel IsVisible="{Binding IsTimelineShow}">
        <views:TimelineView DataContext="{Binding CurrentTimelineViewModel}"/>
    </DockPanel>
</UserControl>
